<% layout('layouts/boilerplate')%>

   <link rel="stylesheet" href="/stylesheets/tradeTile.css">
   <link rel="stylesheet" href="/stylesheets/main.css">

   <%- include('./partials/hero.ejs')%>




      <main>
         <section id="introduction" class="py-2 py-lg-5 d-flex flex-column">
            <div class="container">

               <h2 class="display-5 position-relative" data-aos="fade-right">Our strengths
                  <img loading="lazy" class="enticing-svg right-0" src="/assets/undraw/undraw_be_the_hero_ssr2.svg"
                     alt="Strength Svg Element">
               </h2>
               <p class="lead text-light-white text-justify mb-0 ">
                  We do not beat about the bush! If you wish to see trades
                  highlighted by the community and our moderators you can do it right away!
               </p>
               <a href="#highlights" class="link-light lead ">Click here to see featured ones!</a>

               <div class="animateNumbersContainer mt-5 d-flex flex-row flex-wrap justify-content-evenly">

                  <span class="p-3 mx-1 my-3 mx-md-2 my-md-0 shadow-lg rounded-3 lift-on-hover animateItem">
                     <h3 class="display-6 text-center animateNumbers">
                        <%= skinsCount %>
                     </h3>
                     <p class="mb-0 lead text-center">Skins</p>
                  </span>
                  <span class="p-3 mx-1 my-3 mx-md-2 my-md-0 shadow-lg rounded-3 lift-on-hover animateItem">
                     <h3 class="display-6 text-center animateNumbers">
                        <%= visitorsCount %>
                     </h3>
                     <p class="mb-0 lead text-center">All Visitors</p>
                  </span>
                  <span class="p-3 mx-1 my-3 mx-md-2 my-md-0 shadow-lg rounded-3 lift-on-hover animateItem">
                     <h3 class="display-6 text-center animateNumbers">
                        <%= tradesCount %>
                     </h3>
                     <p class="mb-0 lead text-center">Trades</p>
                  </span>

               </div>
            </div>

         </section>

         <div id="wave1"></div>

         <section id="tiles" class="py-5 py-lg-15">
            <div class="container">

               <h2 class="display-5 position-relative" data-aos="fade-left">
                  Unlimited opportunities
                  <img loading="lazy" class="enticing-svg left-0"
                     src="/assets/undraw/undraw_Investment_data_re_sh9x.svg" alt="Opportunities Svg Element">
               </h2>
               <p class="lead text-light-white text-justify">Lorem ipsum dolor, sit amet consectetur adipisicing elit.
                  Nesciunt rerum
                  modi
                  itaque fugit
                  repellat natus, laboriosam voluptas ex nostrum tempore inventore voluptates beatae. Corrupti mollitia
                  soluta
                  impedit, aperiam repellendus eaque!</p>
               <div class="grid-1 mt-5">

                  <a data-aos="flip-left" data-aos-delay="0" data-aos-anchor=".grid-1" href='/trades?q=random'
                     class="jeden  grey-light_blue-pastell">
                     <span class=" grid-1-content">Show A Random Trade</span>
                     <img loading="lazy" class="grid-1-content-bg" src="/assets/svgs/other/dice-solid.svg" alt="">
                  </a>
                  <a data-aos="flip-right" data-aos-delay="50" data-aos-anchor=".grid-1" href='/favourites'
                     class="dwa  light_blue-blue-pastell">
                     <span class="grid-1-content">Favourites</span>
                     <img loading="lazy" class="grid-1-content-bg" src="/assets/svgs/other/star-half.svg" alt="">
                  </a>
                  <a data-aos="flip-left" data-aos-delay="100" data-aos-anchor=".grid-1" href='/guide'
                     class="trzy blue-purple-pastell">
                     <span class=" grid-1-content">Guide - First Steps</span>
                     <img loading="lazy" class="grid-1-content-bg" src="/assets/svgs/other/info-circle-solid.svg"
                        alt="">
                  </a>
                  <a data-aos="flip-right" data-aos-delay="150" data-aos-anchor=".grid-1" href='/about#collaboration'
                     class="cztery  purple-pink-pastell">
                     <span class=" grid-1-content">Collaboration</span>
                     <img loading="lazy" class="grid-1-content-bg" src="/assets/svgs/other/hands-helping-solid.svg"
                        alt="">
                  </a>
                  <a data-aos="flip-down" data-aos-delay="200" data-aos-anchor=".grid-1" href='/explore'
                     class="piec  pink-red-pastell">
                     <span class=" grid-1-content">Explore Our Engine</span>
                     <img loading="lazy" class="grid-1-content-bg" src="/assets/svgs/other/graph-up.svg" alt="">
                  </a>

               </div>

            </div>
         </section>

         <div id="wave2"></div>

         <section id="highlights" class="py-5 py-lg-15">
            <div class="container">


               <h2 class="d-flex flex-row display-5 position-relative" data-aos="fade-right">Highlighted Trades&nbsp;
                  <img loading="lazy" class="enticing-svg right-0"
                     src="/assets/undraw/undraw_House_searching_re_stk8.svg" alt="Highlights Svg Element">
                  <span class="badge bg-dark d-flex align-items-center">
                     <img loading="lazy" src="/assets/svgs/other/fire.svg" height="23" alt="Fire" class="me-1">
                     <span>Hot</span>
                  </span>
               </h2>
               <p class="lead text-light-white text-justify">Down below you will find some of the
                  trade-ups which we
                  have carefully
                  picked for you. We pay extra attention to make sure that at they time of their release trade-ups are
                  fully
                  doable and rechable for a decent user.
               </p>
               <% if (highlights) { %>
                  <ul>
                     <% let i=1;let directions=['right','left']; %>
                        <% for(let highlightedTrade of highlights){ %>
                           <% let animationData=`data-tilt data-tilt-max="1.2" data-tilt-speed="2000"
                              data-aos="fade-${directions[i%2]}" data-aos-duration="750" data-aos-once="true" ` %>
                              <% ++i; %>



                                 <%-` ${include('./partials/tradeTile.ejs',{
                                    elementName:'li',pricesType:highlightedTrade.pricesType,
                                    title:highlightedTrade.highlightName,statistics:highlightedTrade.orginalTrade.statistics,
                                    data:highlightedTrade.orginalTrade.data,_id:highlightedTrade.orginalTrade._id,
                                    favouritesInfo:highlightedTrade.orginalTrade.favouritesInfo,isHighlighted:true,
                                    showPageLink:'/trades/'+highlightedTrade.orginalTrade._id,isOnFavouritePage:false,
                                    animationData:animationData,})}` %>


                                    <% } %>

                  </ul>
                  <% } %>


            </div>
         </section>

